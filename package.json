{
  "name": "react-html5-camera-photo",
  "version": "1.5.10",
  "description": "React.js HTML5 Camera Photo",
  "main": "build/index.js",
  "scripts": {
    "start1": "node scripts/start.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "buildHtml": "node scripts/buildHtml.js",
    "demo": "node scripts/demo.js",
    "predeployHtml": "npm run buildHtml",
    "deployHtml": "gh-pages -d build",
    "test": "node scripts/test.js --env=jsdom",
    "eslint:src": "eslint --ignore-pattern !**/*.{test,mock}.js src/",
    "eslint:watch": "esw --color --ignore-pattern !**/*.{test,mock}.js src/ -w",
    "eslint:fix": "eslint --fix src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mabelanger/react-html5-camera-photo"
  },
  "author": "Michel-Alexandre Belanger <michel.alexandre.belanger@gmail.com> (https://github.com/mabelanger)",
  "bugs": {
    "url": "https://github.com/mabelanger/react-html5-camera-photo/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "html5",
    "camera",
    "photo",
    "component"
  ],
  "homepage": "https://mabelanger.github.io/react-html5-camera-photo/",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^10.0.1",
    "html-webpack-plugin": "^5.5.0",
    "react-html5-camera-photo": "^1.5.10",
    "react-scripts": "^5.0.1",
    "tesseract.js": "^2.1.5",
    "uglifyjs-3-webpack-plugin": "^1.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.0.0",
    "@types/react-html5-camera-photo": "^1.5.1",
    "autoprefixer": "7.1.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "^8.3.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^4.0.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "gh-pages": "^1.1.0",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react": "^16.8.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.8.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "terser-webpack-plugin": "^5.3.6",
    "url-loader": "0.6.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "postinstall": "patch-package",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "resolutions": {
    "webpack": "4.46.0"
  },
  "babel": {
    "presets": [
      "react-app",
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "max-len": "warn"
        }
      }
    ]
  },
  "files": [
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "CONTRIBUTING.md",
    "build"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
